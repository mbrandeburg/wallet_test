<style type="text/css">
       
    html { 
  background:  url(https://static.pexels.com/photos/34435/pexels-photo.jpg) no-repeat center center fixed; 
  background-size: cover;
}
  </style>
<body>
  <h1 style="font-family:helvetica;"> Listing all Accounts:</h1>
  
  <p style="font-family:helvetica;"> 
  <%= link_to "Create new account", new_account_path %>
  </p>
  <table>
    <tr> <!--table row then table header -->
      <th>Title</th>
      <th>Description</th>
    </tr>
    <% @accounts.each do |account| %>
    <tr>
      <td><%= account.title %></td>
      <td><%= account.description %></td>
      <td><%= link_to "Edit", edit_account_path(account) %></td>
      <td><%= link_to "Show", account_path(account) %></td>
    </tr>
    <% end %>
  </table>
</body>